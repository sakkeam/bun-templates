		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"vite": "^7.1.9",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"class-variance-authority": "^0.7.1",
